<%if(success && success.length) {%>
    <div class="alert alert-success py-2 alert-dismissible fade show text-center"  role="alert">
      <strong ><%=success%></strong>
    </div>
    <%}%>
<%if(failed && failed.length) {%>
      <div class="alert alert-danger py-2 alert-dismissible fade show text-center"  role="alert">
        <strong ><%=failed%></strong>
      </div>
  <%} %>